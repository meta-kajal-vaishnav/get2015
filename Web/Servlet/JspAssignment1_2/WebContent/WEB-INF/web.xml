<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<display-name>RegistrationForm</display-name>
		<servlet-name>RegistrationForm</servlet-name>
		<servlet-class>com.servlets.RegistrationForm</servlet-class>
	</servlet>
	<servlet>
		<display-name>RegistrationFormNext</display-name>
		<servlet-name>RegistrationFormNext</servlet-name>
		<servlet-class>com.servlets.RegistrationFormNext</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationForm</servlet-name>
		<url-pattern>/RegistrationForm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegistrationFormNext</servlet-name>
		<url-pattern>/RegistrationFormNext</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>

